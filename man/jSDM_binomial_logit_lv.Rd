                              site_data=X,
                              n_latent=n_latent, 
                              # Starting values 
                              beta_start=0,
                              lambda_start=0,
                              # Priors 
                              mu_beta=0, V_beta=1.0E6,
                              mu_lambda=0, V_lambda=10,
                              # Various
                              seed=1234, ropt=0.44, verbose=1)

